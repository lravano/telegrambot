{
    "name": "rider/telegrambot",
    "description": "This package is a wrapper for TelegramBot REST API",
    "authors": [
        {
            "name": "Lorenzo Ravano",
            "email": "l.ravano88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": ">=6.0"        
    }, 
    "autoload": {
        "psr-4": {
            "Rider\\Telegrambot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rider\\Telegrambot\\TelegramBotServiceProvider"
            ],
            "aliases": {
                "TelegramBot": "Rider\\Telegrambot\\Facade"
            }
        }
    }   

}
